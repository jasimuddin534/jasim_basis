@{
    ViewBag.Title = "Managers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <section id="list" class="active">
        <div class="row-fluid">
            <div class="span12">
               <div class="page-header" style="position: relative">
                    <img src="~/Images/plinc-logo.png" style="position: absolute; top: 10px; right: 10px;" />
                    <h1>@ViewBag.Title</h1>
                </div>
            </div>
        </div>
        <div class="row-fluid">
            <div class="span9">
                <input type="text" name="search" value="Strat Date" class="input-block-level" />
            </div>
            <div class="span3">
                <input type="text" name="search" value="Strat Date" class="input-block-level" />
            </div>
           @* <div class="span3">
                <button data-bind='click: $root.save' class="btn btn-danger btn-large">Sumbit</button>
                
            </div>*@

        </div>
        <div class="row-fluid">
            <div class="span12 content-body">
                <table id="table1" class="table table-bordered">
                    <thead>
                        <tr>
                            @*<td>Sl No</td>*@
                            <td>Date</td>
                            <td>Invoice No</td>
                            <td>Product Type</td>
                            <td>Brand</td>
                            <td>Model</td>
                            <td>IMIE-1</td>
                            <td>BarCode</td>
                            <td>Supplier</td>
                            <td>Selling Price</td>
                        </tr>
                    </thead>
                    <tbody>
                    @foreach (var item in ViewBag.orders)
                            {
                                //var serialNumber = item.OrderId;
                                var date = item.CreatedTime;
                                var invoiceNo = item.OrderNumber;
                                var productType = "Demo Type";
                                var brand = "Demo Brand";
                                var modelNo = "Demo Model No";
                                var imie1 = "Demo IMIE-1";
                                var barcode = item.BarCode;
                                var supplier = item.CreatedBy;
                                var sellingPrice = item.Price;
                       
                                
                                <tr>
                                    
                                   @* <td>
                                        @serialNumber
                                    </td>*@
                                    <td>
                                        @date
                                    </td>
                                    <td>
                                        @invoiceNo
                                    </td>
                                    <td>
                                        @productType
                                    </td>
                                   <td>
                                       @brand
                                   </td>
                                    <td>
                                        @modelNo
                                    </td>
                                    <td>
                                        @imie1
                                    </td>
                                    <td>
                                        @barcode
                                    </td>
                                    <td>
                                        @supplier
                                    </td>
                                    <td>
                                        @sellingPrice
                                    </td>
                                </tr>     
                           }
                        </tbody>
                </table>
            </div>

        </div>

    </section>
