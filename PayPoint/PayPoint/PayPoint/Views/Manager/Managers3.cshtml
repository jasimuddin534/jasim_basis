                                @sellingPrice
                            </td>
                            <td>
                                @barcode
                            </td>
                            <td>
                                @supplier
                            </td>


                        }

                        @foreach (var item in ViewBag.products3)
                        {

                            var productType = item.Category;
                            var brand = item.Brand;
                            var modelNo = item.Model;
                            var imie1 = item.IMEI1;
                        
                                


                            <td>
                                @productType
                            </td>
                            <td>
                                @brand
                            </td>
                            <td>
                                @modelNo
                            </td>
                            <td>
                                @imie1
                            </td>
                        
                        }
                    </tr>
